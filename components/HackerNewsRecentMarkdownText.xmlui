<Component name="HackerNewsRecentMarkdownText">

  <H1>Recent HackerNews stories</H1>


  <Items data="{$props.data}">
    <Markdown content="[{$item.id}](https://news.ycombinator.com/item?id={$item.id}) {$item.time} {$item.title} by {$item.by} score {$item.score} comments {$item.descendants} [{$item.url}]({$item.url})" />
  </Items>  
    
</Component>

