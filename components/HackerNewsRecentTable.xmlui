<Component name="HackerNewsRecentTable">

    <VStack>
  
      <H1>Recent HackerNews stories</H1>
  
      <Table data="{$props.data}">
        <Column bindTo="id"/>
        <Column bindTo="title"/>
        <Column bindTo="by"/>
        <Column bindTo="time"/>
        <Column bindTo="url"/>
        <Column bindTo="score"/>
        <Column bindTo="descendants"/>
      </Table>

  
    </VStack>
  
  </Component>