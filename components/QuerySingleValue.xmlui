<Component name="QuerySingleValue">
  <DataSource id="newest" url="{$props.url}" body="{$props.body}" method="POST" />
  <HStack>
    <Card title="{$props.title}">
      <Text>{JSON.stringify($props)}</Text>
      <Items data="{newest}">
        <Text>{ $item.single_value } </Text>
      </Items>
    </Card>
  </HStack>
</Component>