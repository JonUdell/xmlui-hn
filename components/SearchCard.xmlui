<Component name="SearchCard">

    <Text>
        { window.search($props.term,10) }
    </Text>

    <Text>
        { typeof(window.search($props.term,10)) }
    </Text>

    
    <DataSource id="data" url="{ window.xmluiHost + '/query' }" 
      rawBody="{ window.search($props.term,10) }" method="POST" />

    <Card gap="0" title="{$props.title}">
        <Items data="{data}">
            <Text>{$item.title}</Text>
        </Items>
    </Card>

</Component>