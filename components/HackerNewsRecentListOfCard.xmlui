<Component name="HackerNewsRecentListOfCard">

  <H1>Recent HackerNews stories</H1>

  <List data="{$props.data}">
    <Card>
      {$item.title} {$item.by} at {$item.time.slice(0, 10)} {$item.url} score: {$item.score} {$item.descendants ? 'comments: ' + $item.descendants : ''}
    </Card>
  </List>

</Component>