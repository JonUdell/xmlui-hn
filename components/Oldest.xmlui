<Component name="Oldest">


    <DataSource id="oldest" url="http://hn.jonudell.info:8081/query" body='{{"sql":"select min(time) as min from hn"}}'
        method="POST" />

        <Card title="oldest">
            <Items data="{oldest}">
                <Text>{$item.min.slice(0,10)}</Text>
            </Items>
        </Card>

</Component>